<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.f"
	xmlns:card="sap.f.cards"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">
    	<!-- <ToggleButton
			id="revealGrid"
			text="Reveal Grid"
			press="onRevealGrid"
			class="sapUiSmallMarginBegin sapUiSmallMarginTop sapUiTinyMarginBottom" /> -->

	<f:GridContainer
		id="gridCar"
		class="sapUiSmallMarginBegin sapUiSmallMarginBottom"
		snapToRow="true"
	>
			<GenericTile header="입차한 차량" frameType="OneByHalf">
				<layoutData>
					<f:GridContainerItemLayoutData minRows="1" columns="2"/>
				</layoutData>
				<TileContent>
					<ObjectNumber
                        id="entryCarCount"
						class="sapMObjectNumberLarge"
                        number=""
                        unit="대"
                    />
				</TileContent>
			</GenericTile>

			<GenericTile header="정기권 차량" frameType="OneByHalf">
				<layoutData>
					<f:GridContainerItemLayoutData minRows="1" columns="2"/>
				</layoutData>
				<TileContent>
					<ObjectNumber
                        id="vipCarCount"
						class="sapMObjectNumberLarge"
                        number=""
                        unit="대"
                    />
				</TileContent>
			</GenericTile>
			<GenericTile header="정산 수입" subheader="20 대" frameType="OneByHalf">
				<layoutData>
					<f:GridContainerItemLayoutData minRows="1" columns="2" />
				</layoutData>
				<TileContent unit="Unit" footer="Footer Text">
					<NumericContent value="12" />
				</TileContent>
			</GenericTile>
	</f:GridContainer>
</core:FragmentDefinition>
        