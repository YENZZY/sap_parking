<!-- 정산된 차량 내역 목록 -->
<Panel>
<OverflowToolbar>
    <Title text="정산 차량 현황" level="H2"/>
    <ToolbarSpacer/>
<Button
    tooltip="Sort"
    icon="sap-icon://sort"
    press="onSort"/>
</OverflowToolbar>
    <Table id="CompleteTable"
        inset="false"
        items="{
            path: 'paidModel>/',
            sorter: {
                path: 'NumberPlate'
            }
        }">
        <columns>
            <Column width="12rem" minScreenWidth="Tablet" demandPopin="true">
            <Text text="차량번호" textAlign="Center"/>
            </Column>
            <Column width="12rem" minScreenWidth="Tablet" demandPopin="true">
            <Text text="차량구분" textAlign="Center"/>
            </Column>
            <Column width="12rem" minScreenWidth="Tablet" demandPopin="true">
            <Text text="차량종류" textAlign="Center"/>
            </Column>
            <Column width="12rem" minScreenWidth="Tablet" demandPopin="true">
            <Text text="출차시간" textAlign="Center"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem vAlign="Middle">
            <cells>
            <Text	text="{paidModel>NumberPlate}" textAlign="Center"/>
            <Text	text="{paidModel>TypeName}" textAlign="Center"/>
            <Text	text="{paidModel>Brand}" textAlign="Center"/>
            <Text	text="{paidModel>ExitTime}" textAlign="Center"/>
            </cells>
            </ColumnListItem>
        </items>
    </Table>
    </Panel>
</core:FragmentDefinition>